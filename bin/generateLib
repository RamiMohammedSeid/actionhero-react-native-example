#!/usr/bin/env node

const Primus = require('primus')
const fs = require('fs')
const path = require('path')

let primus = Primus.createServer(function connection (spark) {}, { port: 8080, transformer: 'websockets' })
const library = primus.library()
fs.writeFileSync(path.join(__dirname, '..', 'lib', 'primus.js'), library)

process.exit(0)
